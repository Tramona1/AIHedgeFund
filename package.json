{
  "name": "ai-hedge-fund",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:lite": "pnpm --filter data-pipeline run dev:lite",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@clerk/nextjs": "^6.12.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@tailwindcss/postcss": "^4.0.9",
    "clsx": "^2.1.0",
    "framer-motion": "^12.4.7",
    "postgres": "^3.4.5",
    "recharts": "^2.15.1",
    "tailwind-merge": "^2.2.1",
    "drizzle-orm": "^0.30.10"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "0.30.10"
    }
  }
}